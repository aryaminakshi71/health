{
  "name": "@healthcare-saas/storage",
  "version": "1.0.0",
  "type": "module",
  "main": "./src/db/schema/index.ts",
  "exports": {
    ".": {
      "types": "./src/db/schema/index.ts",
      "import": "./src/db/index.ts"
    },
    "./db/schema": "./src/db/schema/index.ts",
    "./db/schema/*": "./src/db/schema/*",
    "./redis": "./src/redis/index.ts"
  },
  "scripts": {
    "db:generate": "dotenv -e ../../.env -- drizzle-kit generate",
    "db:push": "dotenv -e ../../.env -- drizzle-kit push",
    "db:migrate": "dotenv -e ../../.env -- drizzle-kit migrate",
    "db:studio": "dotenv -e ../../.env -- drizzle-kit studio"
  },
  "dependencies": {
    "@healthcare-saas/env": "workspace:*",
    "@upstash/redis": "^1.36.1",
    "drizzle-orm": "^0.45.0",
    "drizzle-zod": "^0.8.3",
    "postgres": "^3.4.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.8",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  }
}